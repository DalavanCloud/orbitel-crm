<h1>Houses#check</h1>
<% form_for :house do |f| %>
  <% field_set_tag 'Адрес', :class => 'address' do %>
    <table>
    <tr>
      <td> Улица:
      <td> <%= text_field_with_auto_complete :street, :name, {
          :size => 20,
          :name => 'house[street]',
        }, 
        :url  => auto_complete_streets_path,
        :indicator => 'ai1' %>
      <td> <%= image_tag 'ajax.gif', :style => 'display:none', :id => 'ai1' %>
    <tr>
      <td> Дом:
      <td> <%= f.text_field :number, :size => 5 %>
    </table>
  <% end %>
<% end %>
